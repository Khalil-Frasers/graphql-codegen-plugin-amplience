{
  "name": "amplience-graphql-codegen-json",
  "author": "Lab Digital",
  "version": "0.1.4",
  "private": false,
  "license": "MIT",
  "module": "index.mjs",
  "main": "index.js",
  "typings": "index.d.ts",
  "files": [
    "."
  ],
  "engines": {
    "node": ">=16"
  },
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs"
    }
  },
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup",
    "postbuild": "cp package.json dist",
    "test": "jest",
    "lint": "eslint",
    "prepare": "tsup",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "example": "graphql-codegen --config examples/codegen.yml"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "^2.1.1",
    "amplience-graphql-codegen-common": "*",
    "change-case": "^4.1.2",
    "graphql": "^15.6.0"
  }
}
